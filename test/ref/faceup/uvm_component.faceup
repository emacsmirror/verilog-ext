»  «m://»«x:|    ...
»  «m://»«x:|  endclass
»  «m://»«x:|
»  «m://»«x:|  class mycomp extends uvm_component;
»  «m://»«x:|    `uvm_component_utils(mycomp)
»  «m://»«x:|    ...
»  «m://»«x:|  endclass
»  «m://»«x:|
»  «m://»«x:|  class block extends uvm_component;
»  «m://»«x:|    `uvm_component_utils(block)
»  «m://»«x:|    comp c_inst;
»  «m://»«x:|    virtual function void build_phase(uvm_phase phase);
»  «m://»«x:|      set_inst_override_by_type("c_inst",comp::get_type(),
»  «m://»«x:|                                         mycomp::get_type());
»  «m://»«x:|    endfunction
»  «m://»«x:|    ...
»  «m://»«x:|  endclass
»
